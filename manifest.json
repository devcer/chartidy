{
  "manifest_version": 2,
  "name": "Chartidy",
  "description": "A chrome extension which helps users visualize table data in charts.",
  "version": "0.0.1",
  "icons": {
    "64": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end",
      "js": [
        "micromodal.min.js",
        "Chart.min.js",
        "content_script.js"
      ],
      "css": [
        "micromodal.css",
        "styles.css"
      ]
    }
  ]
}